{
  "name": "kprobe_entry",
  "description": "Instrument PTX at kprobe entry points, excluding __memcapture",
  "attach_points": {
    "includes": ["^kprobe/.*$"] ,
    "excludes": ["^kprobe/__memcapture$"]
  },
  "parameters": {
    "instrumentation_label": "entry",
    "emit_nops_for_alignment": true
  }
}





