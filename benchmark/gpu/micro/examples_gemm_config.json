{
  "output_prefix": "benchmark/gpu/micro/examples_gemm",
  "global_env": {},
  "probe_env": {
    "BPFTIME_LOG_OUTPUT": "console",
    "LD_PRELOAD": "build/runtime/syscall-server/libbpftime-syscall-server.so"
  },
  "agent_env": {
    "BPFTIME_LOG_OUTPUT": "console",
    "LD_PRELOAD": "build/runtime/agent/libbpftime-agent.so"
  },
  "workload_presets": {
    "tiny": "benchmark/gpu/workload/matrixMul 32 100 32 1",
    "small": "benchmark/gpu/workload/matrixMul 1000 100 256 4",
    "medium": "benchmark/gpu/workload/matrixMul 10000 100 256 40",
    "large": "benchmark/gpu/workload/matrixMul 10000 100 512 196",
    "xlarge": "benchmark/gpu/workload/matrixMul 10000 100 512 1954",
    "small_64x16": "benchmark/gpu/workload/matrixMul 1000 100 64 16",
    "minimal": "benchmark/gpu/workload/matrixMul 32 3 32 1"
  },
  "test_cases": [
    {
      "name": "Baseline (tiny)",
      "probe_binary_cmd": "",
      "workload": "tiny"
    },
    {
      "name": "Baseline (small)",
      "probe_binary_cmd": "",
      "workload": "small"
    },
    {
      "name": "Baseline (medium)",
      "probe_binary_cmd": "",
      "workload": "medium"
    },
    {
      "name": "Baseline (large)",
      "probe_binary_cmd": "",
      "workload": "large"
    },
    {
      "name": "Baseline (minimal)",
      "probe_binary_cmd": "",
      "workload": "minimal"
    },
    {
      "name": "CUDA Counter (minimal)",
      "probe_binary_cmd": "example/gpu/cuda-counter/cuda_probe",
      "workload": "minimal",
      "baseline": "Baseline (minimal)"
    },
    {
      "name": "Memory Trace (small)",
      "probe_binary_cmd": "example/gpu/mem_trace/mem_trace",
      "workload": "small",
      "baseline": "Baseline (small)"
    },
    {
      "name": "Thread Histogram (small)",
      "probe_binary_cmd": "example/gpu/threadhist/threadhist",
      "workload": "small",
      "baseline": "Baseline (small)"
    },
    {
      "name": "Launch Latency (minimal)",
      "probe_binary_cmd": "example/gpu/launchlate/launchlate benchmark/gpu/workload/matrixMul:_Z16cudaLaunchKernelIcE9cudaErrorPT_4dim3S3_PPvmP11CUstream_st",
      "workload": "minimal",
      "baseline": "Baseline (minimal)"
    },
    {
      "name": "Kernel Retsnoop (small)",
      "probe_binary_cmd": "example/gpu/kernelretsnoop/kernelretsnoop",
      "workload": "small",
      "baseline": "Baseline (small)"
    }
  ]
}
